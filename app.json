{
  "name": "Challenge Days",
  "description": "A typical Heroku Connect app",
  "repository": "https://github.com/Lajule/challenge-days",
  "logo": "http://challenge-days.herokuapp.com/images/challenge-days.png",
  "keywords": [
    "node",
    "express",
    "pug",
    "node-postgres"
  ],
  "addons": [
    "heroku-postgresql:hobby-dev",
    "herokuconnect:demo"
  ],
  "env": {
    "LEVEL": {
      "description": "The logger severity level.",
      "value": "info"
    },
    "SECRET": {
      "description": "A secret key for verifying the integrity of signed cookies.",
      "generator": "secret"
    },
    "PORT": {
      "description": "The web server listening port.",
      "value": "8080"
    }
  },
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ]
}
